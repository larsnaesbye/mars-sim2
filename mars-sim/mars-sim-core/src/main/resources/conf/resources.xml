<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE resource-list SYSTEM "conf/dtd/resources.dtd">

<!-- List of all amount resources in the simulation. -->
<resource-list>

	<!-- Potable water -->
	<!-- name - the name of the resource. -->
	<!-- phase - the normal phase of the resource as used. -->
	<!--         Possible values: "solid", "liquid" or "gas" -->
	<!-- life-support - Is the resources used for life support? -->
	<resource name="water" phase="liquid" life-support="true">potable water</resource>

	<resource name="oxygen" phase="gas" life-support="true" />
	<resource name="hydrogen" phase="gas" />
	<resource name="methane" phase="gas" />
	<resource name="carbon dioxide" phase="gas" />
	<resource name="carbon monoxide" phase="gas" />
<!-- <resource name="chlorine" phase="gas"/> -->

	<resource name="food" phase="solid" life-support="true">Represents general freeze dried food.</resource>

	<resource name="Soybeans" phase="solid" life-support="true">Soybean seeds.</resource>
	<resource name="Soymilk" phase="liquid" life-support="true">Plant milk produced by soaking dry soybeans and grinding them with water.</resource>
	<resource name="Soybean Oil" phase="liquid" life-support="true">Oil derived from soybeans.</resource>
	<resource name="Tofu" phase="solid" life-support="true">Tofu made of Okara from soybeans.</resource>
	<resource name="Soy Flour" phase="solid" life-support="true">Flour made from ground soybeans.</resource>
	<resource name="Soy Fiber" phase="solid" life-support="true">Light yellow, silky protein soybeans fiber .</resource>
	<resource name="Soy Protein" phase="solid" life-support="true">Protein extract derived from soybeans.</resource>
	<resource name="Soybean Ash" phase="solid" life-support="true">Soybean extract composed of mainly Potassium, Phosphorus, and Calcium</resource>
	<resource name="Okara" phase="solid" life-support="true">The Soy Pulp consisting of insoluble parts of the soybean which
		 after pureed soybeans are filtered in the production of soymilk and tofu.</resource>
	<resource name="Soy Sprouts" phase="solid" life-support="true">Sprouted soybeans.</resource>

	<resource name="Sodium Hypochlorite" phase="solid" life-support="true">For sanitization of soybeans.</resource>
	<resource name="Yeast" phase="solid" life-support="true">For making bread.</resource>


	<resource name="Grain Group" phase="solid" life-support="true">Whole Grain such as wheat, rice, oats, cornmeal, and barley, etc.;
		Small hard dry seeds, with or without attached hulls or fruit layers</resource>
	<resource name="Fruit Group" phase="solid" life-support="true">
		the fleshy seed-associated structures of a plant that are sweet or sour and edible in the raw state</resource>
	<resource name="Vegetable Group" phase="solid" life-support="true">the leafy or root structure of a plant cultivated for food</resource>
	<resource name="Legume Group" phase="solid" life-support="true">the seed of a plant cultivated for food</resource>
	<resource name="Spice Group" phase="solid" life-support="true">For food seasoning</resource>


	<resource name="rock samples" phase="solid">Rock samples collected from explored sites.</resource>
	<resource name="waste water" phase="liquid">Sewage water used as crop fertilizer.</resource>
	<resource name="ice" phase="solid">Ice-rich permafrost soil</resource>
	<resource name="regolith" phase="solid">Martian soil</resource>

	<resource name="iron oxide" phase="solid">also known as rust</resource>

	<resource name="carbon" phase="solid">Carbon powder</resource>
	<resource name="sand" phase="solid">Martian soil with iron oxide removed. Used for making glass.</resource>

	<resource name="iron powder" phase="solid" />
	<resource name="aluminum oxide" phase="solid" />
	<resource name="ethylene" phase="gas" />

	<resource name="polyethylene" phase="solid">Polyethylene pellets or flakes.</resource>

	<resource name="gypsum" phase="solid" />
	<resource name="calcium carbonate" phase="solid" />
	<resource name="lime" phase="solid" />
	<resource name="styrene" phase="solid" />
	<resource name="polyester resin" phase="liquid" />

	<!-- Mineral concentrations. -->
	<resource name="hematite" phase="solid" />
	<resource name="magnetite" phase="solid" />
	<resource name="magnesite" phase="solid" />
	<resource name="olivine" phase="solid" />
	<resource name="goethite" phase="solid" />
	<resource name="kamacite" phase="solid" />
	<resource name="taenite" phase="solid" />
	<resource name="chalcopyrite" phase="solid" />
	<resource name="malachite" phase="solid" />
	<resource name="sylvite" phase="solid" />

	<resource name="nickel" phase="solid" />
	<resource name="sulfur" phase="solid" />
	<resource name="magnesium" phase="solid" />
	<resource name="silicon" phase="solid" />
	<resource name="copper" phase="solid" />

	<resource name="potash lye" phase="solid">Potassium hydroxide</resource>

	<resource name="cement" phase="solid" />
	<resource name="concrete" phase="solid" />
	<resource name="mortar" phase="solid" />

</resource-list>
